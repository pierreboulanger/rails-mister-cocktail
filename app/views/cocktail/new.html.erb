<div class="container">
  <div class="text-center">
    <h1>Create a new Cocktail !</h1>

    <%= simple_form_for @cocktail, :url => cocktail_index_path do |f| %>
      <%= f.input :name %>
      <%= f.input :photos, as: :attachinary %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>


<%= link_to "Back", cocktail_index_path %>
